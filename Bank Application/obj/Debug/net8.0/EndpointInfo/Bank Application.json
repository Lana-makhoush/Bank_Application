    "/api/AccountType/add": {
      "post": {
        "tags": [
          "AccountType"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "AnnualInterestRate",
                  "DailyWithdrawalLimit",
                  "MonthlyFee",
                  "TermsAndConditions",
                  "TypeName"
                ],
                "type": "object",
                "properties": {
                  "TypeName": {
                    "maxLength": 30,
                    "pattern": "^[\\u0621-\\u064A a-zA-Z]+$",
                    "type": "string"
                  },
                  "AnnualInterestRate": {
                    "minimum": 50,
                    "type": "number",
                    "format": "double"
                  },
                  "DailyWithdrawalLimit": {
                    "minimum": 200,
                    "type": "number",
                    "format": "double"
                  },
                  "MonthlyFee": {
                    "minimum": 100,
                    "type": "number",
                    "format": "double"
                  },
                  "TermsAndConditions": {
                    "maxLength": 500,
                    "minLength": 10,
                    "pattern": "^[\\u0621-\\u064A a-zA-Z0-9\\s.,\\-]*$",
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "TypeName": {
                  "style": "form"
                },
                "AnnualInterestRate": {
                  "style": "form"
                },
                "DailyWithdrawalLimit": {
                  "style": "form"
                },
                "MonthlyFee": {
                  "style": "form"
                },
